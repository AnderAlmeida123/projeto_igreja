<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>Page Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="stylesheet" type="text/css" media="screen" href="main.css" />
    <script src="main.js"></script>
  </head>
  <body>
    <h1>
      <p>Setores</p>
    </h1>
    <table>
      <tr>
        <th>Descrição</th>
        <th>Nome do Setor</th>
        <th>Responsavel pelo setor</th>
        <th>Comunidade</th>
      </tr>
      <% dados.forEach(function(setor){ %>
      <tr>
        <td><%= setor.descricao %></td>
        <td><%= setor.setor %></td>
        <td><%= setor.responsavelId %></td>
        <td><%= setor.comunidadeId %></td>
      </tr>
      <% }) %>
    </table>
  </body>
</html>
